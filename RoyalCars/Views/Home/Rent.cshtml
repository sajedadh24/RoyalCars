@model IEnumerable<Reserve>

@{
    ViewData["Title"] = "Rent";
}

@foreach (var item in Model)
{







    @if (item.IsActive == true)
    {
        <!-- Plain Text Start -->
        <div>
            <h4 class="d-inline-block text-primary text-uppercase border-bottom border-5 mb-4">@item.FirstName</h4>
            <div class="bg-light rounded text-center" style="padding: 30px;">
                <h1 style="color:aqua">Reservation DONE</h1>
                <p>DoctorName: @item.MobileNumber</p>
                <p>Date:@item.Lastame </p>
                <h2 class="btn btn-primary py-2 px-4">wolcome</h2>
            </div>
        </div>
        <!-- Plain Text End -->
    }
    else
    {
<div>
    <h4 class="d-inline-block text-primary text-uppercase border-bottom border-5 mb-4">@item.FirstName</h4>
    <div class="bg-light rounded text-center" style="padding: 30px;">
        <h1 style="color:aqua">Reservation not DONE</h1>
        <p>DoctorName: @item.MobileNumber</p>
        <p>Date:@item.Lastame </p>
        <h2 class="btn btn-primary py-2 px-4">wolcome</h2>
    </div>
</div>
    }






}
<a asp-action="Index" asp-controller="Home" class="btn btn-primary">Back To Home</a>
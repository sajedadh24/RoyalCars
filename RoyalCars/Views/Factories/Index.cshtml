@model IEnumerable<RoyalCars.Models.Factory>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FactoryName)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FactoryName)
            </td>
          
            <td>
                <a asp-action="Edit" asp-route-id="@item.FactoryId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FactoryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FactoryId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
